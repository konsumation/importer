{
  "name": "konsum-importer",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "{{main}}"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Konstantin Karpman"
    }
  ],
  "scripts": {
    "cover": "--overwrite-- c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "docs": "--overwrite-- documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "--overwrite-- documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.5.0",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konsumation/konsum-importer.git"
  },
  "bugs": {
    "url": "https://github.com/konsumation/konsum-importer/issues"
  },
  "homepage": "https://github.com/konsumation/konsum-importer#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github-action",
      "konsumation/template"
    ]
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "konsum-db": "^3.4.0",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0"
  }
}
